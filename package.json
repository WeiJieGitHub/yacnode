{
  "name": "yacnode",
  "version": "0.0.1",
  "description": "yet anthor cnode client",
  "main": "index.js",
  "scripts": {
    "_unit-test-lint": "eslint test/unit/specs/*spec.js* test/unit/resource/**/*.js* --config eslintrc.test.js",
    "_functional-test-lint": "eslint test/functional/specs/*spec.js test/functional/assertions/**/*.js --config eslintrc.test.js",
    "_karma-single": "karma start test/unit/karma.config.js --single-run",
    "test-unit": "npm run _unit-test-lint & npm run _karma-single",
    "test-functional": "npm run _functional-test-lint & nightwatch --config test/functional/nightwatch.config.js --env chrome",
    "build": "webpack -p --progress --profile --colors --config build/webpack.production.config.js",
    "dev": "webpack-dev-server --hot --inline  --config build/webpack.dev.config.js",
    "karma": "karma start test/unit/karma.config.js --auto-watch",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "redux",
    "cnode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WeiJieGitHub/yacnode.git"
  },
  "author": "weijie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.29.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.3",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.3",
    "nightwatch": "^0.9.14",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "selenium-server": "^3.4.0",
    "style-loader": "^0.16.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "history": "^4.6.1",
    "modularscale-sass": "^3.0.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0"
  }
}
